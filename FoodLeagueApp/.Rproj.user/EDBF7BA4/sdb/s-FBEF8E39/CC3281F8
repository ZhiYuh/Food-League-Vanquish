) %>% \n     clearShapes() %>%\n     addCircles(~longitude, ~latitude, radius=300,\n                stroke=FALSE, fillOpacity=0.4, fillColor=pal(colorData)) %>%\n     addLegend(\"bottomleft\", pal=pal, values=colorData, title=colorBy,\n              layerId=\"colorLegend\")\n  })\n\n  \n  \n}",
